# Keystore configuration for JaBook app signing
# Copy this file to keystore.properties and fill in your actual values

# Keystore file path (relative to project root)
storeFile=keystore/jabook-release-key.keystore

# Keystore password
storePassword=your_keystore_password

# Key alias
keyAlias=jabook-key

# Key password
keyPassword=your_key_password

# Enable V3 signing (recommended for Android 9+)
enableV3Signing=true

# Enable V4 signing (recommended for Android 11+)
enableV4Signing=true